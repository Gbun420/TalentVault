{
  "kind": "collectionType",
  "collectionName": "contact_unlocks",
  "info": {
    "singularName": "contact-unlock",
    "pluralName": "contact-unlocks",
    "displayName": "Contact Unlock"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "employer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::employer.employer",
      "inversedBy": "contactUnlocks"
    },
    "candidate": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::candidate-profile.candidate-profile",
      "inversedBy": "unlocks"
    },
    "payment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::payment.payment"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "unlocked",
        "revoked"
      ],
      "default": "unlocked"
    },
    "note": {
      "type": "text"
    },
    "unlockedAt": {
      "type": "datetime"
    }
  }
}
